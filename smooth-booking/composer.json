{
    "name": "smoothbooking/smooth-booking",
    "description": "Smooth Booking plugin bootstrapper",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "SmoothBooking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmoothBooking\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    }
}
